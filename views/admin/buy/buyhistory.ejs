<%- include("../../layout/admin-navbar") -%>



<div class="management">
    <br>
    <p class="title">المشتريات</p>
    <button class="show" onclick="window.location.href='/admin/buyhistory/newlist'"><svg xmlns="http://www.w3.org/2000/svg"
        class="icon icon-tabler icon-tabler-plus" width="36" height="36" viewBox="0 0 24 24"
        stroke-width="1.5" stroke="#ffffff" fill="none" stroke-linecap="round" stroke-linejoin="round">
        <path stroke="none" d="M0 0h24v24H0z" fill="none" />
        <line x1="12" y1="5" x2="12" y2="19" />
        <line x1="5" y1="12" x2="19" y2="12" />
    </svg></button>
    <br>
    <br>
    <br>
    <div class="soldHistory">
        <% buyData.forEach(data => {%>
            <div class="folder" onclick="window.location.href='/admin/buyhistory/get-one/<%= data.id%>'">
                <ion-icon class="folder-icon" name="folder"></ion-icon>
                <p><%= data.Date%></p>
            </div>
        <%}) %>
    </div>
</div>

